                                    <div class="govuk-summary-list__row govuk-summary-list__row--no-border">
                                      <dt class="govuk-summary-list__value"><p>Term or acronym</p></dt>
                                      <dd class="govuk-summary-list__value content_review_length"><p>{{ data.text }} </p></dd>
                                    </div>
                                    <div class="govuk-summary-list__row govuk-summary-list__row--no-border">
                                      <dt class="govuk-summary-list__value"><p>Definition</p></dt>
                                      <dd class="govuk-summary-list__value content_review_length"><p>{{ data.value }}</p></dd>
                                    </div>
