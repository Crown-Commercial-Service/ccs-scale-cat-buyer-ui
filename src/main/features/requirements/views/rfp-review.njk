                                    {% if(agreement_id == "RM6187") %}
                                    <h3 class="govuk-!-margin-bottom-0"><strong>Terms and acronyms (Optional)</strong></h3>
                                    {% else %}
                                    <h3 class="govuk-!-margin-bottom-0"><strong>key Terms and acronyms (Optional)</strong></h3>
                                    {% endif %}
                                  </dt>
                                  <dd class="govuk-summary-list__value">&nbsp;</dd>
                                  <dd class="govuk-summary-list__actions">
                                    {% if eventStatus != "published" and closeStatus!="CLOSED" and closeStatus!="COMPLETE" and closeStatus!="EVALUATED" and closeStatus!="AWARDED" %}
                                      <a
                                        class="govuk-link"
                                        href="/rfp/questions?agreement_id={{ agreementId_session }}&proc_id={{ proc_id }}&event_id={{ event_id }}&id=Criterion 3&group_id=Group 3">
                                        Change<span class="govuk-visually-hidden">project name</span>
                                      </a>
                                    {% endif %}
                                  </dd>
                                </div>
                                {% if(termAndAcr !=undefined and  termAndAcr !=null and termAndAcr | length > 0) %}
