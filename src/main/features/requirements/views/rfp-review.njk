{% extends "template.njk" %}
{% from "macros/csrf.njk" import csrfProtection %}

{% set title = 'FC review | Crown Commercial Service' %}
{% set numberWordBig = [
                'Zero',
                'First',
                'Second',
                'Third',
                'Fourth',
                'Fifth',
                'Sixth',
                'Seventh',
                'Eight',
                'Ninth',
                'Tenth'
              ] %}

{% set numberWordSmall = [
                'Zero',
                ' first',
                ' second',
                ' third',
                ' fourth',
                ' fifth',
                ' sixth',
                ' seventh',
                ' eight',
                ' ninth',
                ' tenth'
              ] %}

{% block breadCrumb %}
  {{ CCSBreadcrumbs({
        items: data.breadCrumbs
      }) }}
{% endblock %}

{% block content %}
  <main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">

    {% if (viewError == true) and (apiError == false) %}
      {{ CCSErrorSummary({
        titleText: "There is a problem with publishing RFP",
        errorList: [
          {
            text: "You must check this box to confirm that I have read and confirm the statements above",
            href: "#ccs_rfp_publish_form"
          }
        ]
      }) }}
    {% endif %}

    {% if (checkboxerror==1) %}
      {{ CCSErrorSummary({
        titleText: "There has been an error publishing your FC",
        errorList: [
          {
            text: "Confirmation checkbox must be checked before progressing",
            href: "#ccs_rfp_publish_form"
          }
        ]
      }) }}
    {% endif %}

    

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full">
        <h1 class="govuk-heading-xl">
          <strong>{{data.title }}<strong>
        </h1>
      </div>
    </div>

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full govuk-grid-column-two-thirds-from-desktop">
        {% include "includes/project_header.njk" %}
        <br><br>
        <p class="govuk-body">{{data.subtitle}}
        </p><br><br>
        {{ CCSButton({
    text: "Export your draft one-stage further competition",
    classes: "govuk-button--secondary",
    href: "/rfp/review?download=1"
    }) }}<br><br>

      </div>
    </div>

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full"></div>
    </div>

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full">
        <h2 class="govuk-heading-m">1. Project and colleagues</h2>
        <dl class="govuk-summary-list">
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
         <p> Name your project</p>
        </dt>
            {% if project_name | length > 0 %}
              <dd class="govuk-summary-list__value">
                <p class="govuk-body-s">
                   {{ project_name | safe }} 
                </p>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          You have not given a custom name to your project
        </dd>
            {% endif %}

            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/name-your-project">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Change who is going to lead the procurement</p>
        </dt>
            <dd class="govuk-summary-list__value">
              <ul class="govuk-list govuk-list--none">
                <li>
                  <span class="govuk-body-s">{{procurementLead.name }}</span></li>
                <li>
                  <span class="govuk-body-s">{{procurementLead.email }}</span></li>
                <li>
                  <span class="govuk-body-s">{{procurementLead.telephone }}</span></li>
              </ul> 
            </dd>
            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/procurement-lead">
            Change<span class="govuk-visually-hidden"> procurement lead</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Add colleagues</p>
        </dt>
            {% if procurementColleagues | length > 0 %}
              <dd class="govuk-summary-list__value">
                <ul class="govuk-list govuk-list--none">
                  {% for collaborator in procurementColleagues %}
                    <li>
                      <ul class="govuk-list govuk-list--none">
                        <li>
                          <span class="govuk-body-s">{{collaborator.name}}</span></li>
                        <li>
                          <span class="govuk-body-s">{{collaborator.email}}</span></li>
                        <li>
                          <span class="govuk-body-s">{{collaborator.telephone}}</span></li>
                      </ul>
                    </li>
                  {%- endfor %}
                </ul>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          You have not added anyone so far in your team
        </dd>
            {% endif %}
            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/add-collaborators">
            Change<span class="govuk-visually-hidden"> eoi team members</span>
              </a>
              {% endif %}
            </dd>
          </div>
        </dl>
         <h2 class="govuk-heading-m">2. Upload pricing schedules and any other supporting documents </h2>
         <dl class="govuk-summary-list">
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Upload your pricing schedule</p>
        </dt>
            {% if document | length > 0 %}
              <dd class="govuk-summary-list__value">
                <p class="govuk-body-s">
                  {{ document | safe }}
                </p>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          
        </dd>
            {% endif %}

            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/upload-attachment">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Upload more documents (optional)</p>
        </dt>
            {# <dd class="govuk-summary-list__value"> #}
              {% if documents | length > 0 %}
              <dd class="govuk-summary-list__value">
                <ul class="govuk-list govuk-list--none">
                  {% for doc in documents %}
                    <li>
                       <span class="govuk-body-s">{{doc}}</span>
                       </li>
                  {%- endfor %}
                </ul>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          
        </dd>
            {% endif %}
            {# </dd> #}
            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/upload-doc">
            Change<span class="govuk-visually-hidden"> procurement lead</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Confirm if the IR35 off-payroll working rules apply</p>
        </dt>
            {% if ir35 | length > 0 %}
              <dd class="govuk-summary-list__value">
                <p class="govuk-body-s">
                  {{ ir35 | safe }}
                </p>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          
        </dd>
            {% endif %}
            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/IR35">
            Change<span class="govuk-visually-hidden"> eoi team members</span>
              </a>
              {% endif %}
            </dd>
          </div>
        </dl>
        <h2 class="govuk-heading-m">3. Add context to your further competition </h2>
        <dl class="govuk-summary-list">
        <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
         <strong>Terms and acronyms (Optional) </strong>
         <br/><br/>
         {% for data in termAndAcr %}
          <p>Term or acronym</p><p>Definition</p><br/>
         {% endfor%}
          
        </dt>
        <dd class="govuk-summary-list__value">
          <br/><br/>
          {% for data in termAndAcr %}
          <p>{{data.text}}</p><p>{{data.value}}</p><br/>
          {% endfor%}
          
          {#<p>Expression or acronym answer</p><p>Definition answer</p><br/>
          <p>Expression or acronym answer</p><p>Definition answer</p><br/>#}
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 3">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <strong>Background and context to your requirement</strong><br/><br/>
          {% for data in backgroundArr%}
          <p> Key background information</p></br>
          <p> Social value, economic and enviornmental benefits</p>
          {% endfor %}
        </dt>
        <dd class="govuk-summary-list__value">
        <br/>
          {% for data in backgroundArr%}
          <p>{{data.v1}}</p><br/><br/>
          <p>{{data.v2}}</p>
          {% endfor %}
          
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 4">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p> The business problem you need to solve</p></br>
        </dt>
        <dd class="govuk-summary-list__value">
          <p> {{businessProbAns}} </p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 5">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
         <strong> The people who will use your product or service (optional)</strong><br/><br/>
          {% for item in keyUser %}
          <p> Service user type</p><p>Details about them</p><br/>
          {% endfor %}
          {#<p> Key user</p><p>definition</p><br/>#}
        </dt>
        <dd class="govuk-summary-list__value">
          <br/><br/>
          {% for item in keyUser %}
          <p> {{item.text}}</p><p>{{item.value}}</p><br/>
          {% endfor %}
          {#<p> Key user answer</p><p>definition answer</p><br/>#}
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 6">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Any work that has been done so far (optional)</p>
        </dt>
        <dd class="govuk-summary-list__value">
          <p>{{workcompletedsofar}}</p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 7">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Which phase the project is currently in</p>
        </dt>
        <dd class="govuk-summary-list__value">
          <p>{{currentphaseofproject}}</p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 8">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Which phase(s) of the project you need resource for</p>
        </dt>
        <dd class="govuk-summary-list__value">
        {% for data in phaseResource %}
          <p>{{data}}</p>
          {% endfor %}
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 9">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <strong>The expected duration of the project </strong><br/><br/>
          <p>The date you want the project to start</p><br/><p>How long you think the project will run for (optional)</p><br/>
        </dt>
        <dd class="govuk-summary-list__value">
        <br/><br/><br/>
          <p>{{startdate}}</p><br/><p>{{indicativeduration}}</p><br/>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 10">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <strong>Who the buying organisation is (optional)</strong><br/><br/>
          <p>Name of the organisation doing the procurement</p><br/><p>Name of the organisation using the products or services</p>
        </dt>
        <dd class="govuk-summary-list__value">
          <br/><br/>
          <p>{{buyingorg1}}</p><br/><p>{{buyingorg2}}</p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 11">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Describe any pre-market engagement you've done (optional) </p>
        </dt>
        <dd class="govuk-summary-list__value">
          <p> {{summarize}}</p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 14">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Choose if this will be a new, replacement or expanded service or product</p>
        </dt>
        <dd class="govuk-summary-list__value">
          <p> {{newreplace}}</p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 15">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Tell us if there is an existing supplier?</p>
        </dt>
        <dd class="govuk-summary-list__value">
          <p> {{incumbentoption}}</p><p>{{suppliername}}</p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 16">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Management information and reporting requirements</p>
        </dt>
        <dd class="govuk-summary-list__value">
          <p> {{managementinfo}}</p>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 17">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <strong>Define your service levels and KPIs</strong><br/><br/>
          {% set i=1 %}
          {% for data in serviceLevel %}
          {% set text=numberWordBig[i] + " service level requirement/KPI" %}
          <p><strong>{{text}}</strong></p>
          <p>Name of requirement</p><br/>
          <p>Description of the criteria</p><br/>
          <p>Your success target</p><br/><br/>
          {% set i=i+1 %}
          {% endfor %}
          {#<p>Enter the service area</p><br/>
          <p>Enter the service area/ KPI description</p><br/>
          <p>Insert the required target</p><br/>#}
        </dt>
        <dd class="govuk-summary-list__value">
          <br/><br/>
          {% for data in serviceLevel%}
          <br/><br/>
          <p>{{data.text}}</p><br/>
          <p>{{data.value}}</p><br/>
          <p> % required target</p><br/><br/>
          {% endfor %}
          {#<p>Service area answer</p><br/>
          <p>Enter the service area/ KPI description</p><br/>
          <p>XX% required target</p><br/><br/>#}
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 19">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <strong>Detail any performance incentives and exit strategies</strong><br/><br/>
          <p>Details of any performance incentives or expected actions for poor performance</p><br/>
          <p>Details about any existing strategy that you would apply if a supplier performed poorly, and you needed to end their contract early</p><br/>
        </dt>
        <dd class="govuk-summary-list__value">
          <br/><br/>
          <p>{{incentive1}}</p><br/><br/>
          <p>{{incentive2}}</p><br/>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 20">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <strong>Contract value and how the suppliers will meet the project needs withing this budget (optional)</strong> <br/><br/>
          <p>Maximum estimated contract value (optional)</p><br/>
          <p>Minimum estimated contract value (optional)</p><br/>
          <p>Ask the supplier how they will meet the project needs within this budget (optional)</p><br/>
          <p>More detail for your question (optional)</p><br/>
          <p>What a good answer looks like (optional)</p><br/>
        </dt>
        <dd class="govuk-summary-list__value">
          <br/><br/><br/>
          <p>{{bc1}}</p><br/>
          <p>{{bc2}}</p><br/>
          <p></p><br/>
          <p></p><br/>
          <p></p><br/>
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 21">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <strong>Enter your requirements</strong><br/><br/>
          {% set i=1 %}
          {% for data in reqGroup%}
          {% set text = "Name of the" +numberWordSmall[i]+" requirement group"%}
          <p>{{text}}</p>
          <p>Name of the requirement</p>
          <p>Description of the requirement</p><br/>
          {% set i=i+1 %}
          {% endfor %}
          {#<p>Requirement title</p>
          <p>Requirement description</p><br/><br/>
          <p>Requirement group</p>
          <p>Requirement title</p>
          <p>Requirement description</p><br/><br/>
          <p>Requirement group</p>
          <p>Requirement title</p>
          <p>Requirement description</p><br/><br/>#}
        </dt>
        <dd class="govuk-summary-list__value">
          <br/><br/>
          {% for data in reqGroup%}
          <p>{{data.title}}</p>
          <p>{{data.group}}</p>
          <p>{{data.desc}}</p><br/>
          {% endfor %}
          {#<p>Answer</p>
          <p>Answer</p><br/><br/>
          <p>Answer</p>
          <p>Answer</p>
          <p>Answer</p><br/><br/>
          <p>Answer</p>
          <p>Answer</p>
          <p>Answer</p><br/><br/>#}
        </dd>
        <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 24">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          </dl>
        <h2 class="govuk-heading-m">4. People, skills, security and location </h2>
        <dl class="govuk-summary-list">
        <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
         <strong> Choose the roles you need </strong><br/><br/>
         <p>Total number of roles added</p><br/>
         <p>Number of roles by job section, family and individual role</p>
        </dt>
        <dd class="govuk-summary-list__value">
