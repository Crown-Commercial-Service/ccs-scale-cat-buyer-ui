{% extends "template.njk" %}
{% from "macros/csrf.njk" import csrfProtection %}

{% set title = 'FC review | Crown Commercial Service' %}
{% set numberWordBig = [
                'Zero',
                'First',
                'Second',
                'Third',
                'Fourth',
                'Fifth',
                'Sixth',
                'Seventh',
                'Eight',
                'Ninth',
                'Tenth'
              ] %}

{% set numberWordSmall = [
                'Zero',
                ' first',
                ' second',
                ' third',
                ' fourth',
                ' fifth',
                ' sixth',
                ' seventh',
                ' eight',
                ' ninth',
                ' tenth'
              ] %}

{% block breadCrumb %}
  {{ CCSBreadcrumbs({
        items: data.breadCrumbs
      }) }}
{% endblock %}

{% block content %}
  <main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">

    {% if (viewError == true) and (apiError == false) %}
      {{ CCSErrorSummary({
        titleText: "There is a problem with publishing RFP",
        errorList: [
          {
            text: "You must check this box to confirm that I have read and confirm the statements above",
            href: "#ccs_rfp_publish_form"
          }
        ]
      }) }}
    {% endif %}

    {% if (checkboxerror==1) %}
      {{ CCSErrorSummary({
        titleText: "There has been an error publishing your FC",
        errorList: [
          {
            text: "Confirmation checkbox must be checked before progressing",
            href: "#ccs_rfp_publish_form"
          }
        ]
      }) }}
    {% endif %}

    

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full">
        <h1 class="govuk-heading-xl">
          <strong>{{data.title }}<strong>
        </h1>
      </div>
    </div>

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full govuk-grid-column-two-thirds-from-desktop">
        {% include "includes/project_header.njk" %}
        <br><br>
        <p class="govuk-body">{% if selectedeventtype=='DA' %}{{data.subtitleDA}}{%else%}{{data.subtitle}}{%endif%}
        </p><br><br>
        {%if(selectedeventtype=='DA')%}
        {{ CCSButton({
    text: "Export your draft",
    classes: "govuk-button--secondary",
    href: "/rfp/review?download=1"
    }) }}
    {%else%}
    {{ CCSButton({
    text: "Export your draft further competition",
    classes: "govuk-button--secondary",
    href: "/rfp/review?download=1"
    }) }}
    {%endif%}<br><br>

      </div>
    </div>

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full"></div>
    </div>
{# --------1 Project and colleagues---START------  #}
{%block Project_And_colleagues%}
    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full">
        <h2 class="govuk-heading-m">1. Project and colleagues</h2>
      <dl class="govuk-summary-list">
        <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
            </dt>
              <dd class="govuk-summary-list__value">
              </dd>
            <dd class="govuk-summary-list__actions">
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
         <p> Name your project</p>
        </dt>
            {% if project_name | length > 0 %}
              <dd class="govuk-summary-list__value">
                <p class="govuk-body-s">
                   {{ project_name | safe }} 
                </p>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          You have not given a custom name to your project
        </dd>
            {% endif %}

            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/name-your-project">
            Change<span class="govuk-visually-hidden">project name</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <p>Change who will lead the procurement</p>
        </dt>
            <dd class="govuk-summary-list__value">
              <ul class="govuk-list govuk-list--none">
                <li>
                  <span class="govuk-body-s">{{procurementLead.name }}</span></li>
                <li>
                  <span class="govuk-body-s">{{procurementLead.email }}</span></li>
                <li>
                  <span class="govuk-body-s">{{procurementLead.telephone }}</span></li>
              </ul> 
            </dd>
            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/procurement-lead">
            Change<span class="govuk-visually-hidden"> procurement lead</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <p>
            Add colleagues to your project 
            </p>
        </dt>
            {% if procurementColleagues !=undefined and procurementColleagues !=null and procurementColleagues | length > 0 %}
              <dd class="govuk-summary-list__value">
                <ul class="govuk-list govuk-list--none">
                  {% for collaborator in procurementColleagues %}
                    <li>
                      <ul class="govuk-list govuk-list--none">
                        <li>
                          <span class="govuk-body-s">{{collaborator.name}}</span></li>
                        <li>
                          <span class="govuk-body-s">{{collaborator.email}}</span></li>
                        <li>
                          <span class="govuk-body-s">{{collaborator.telephone}}</span></li>
                      </ul>
                    </li>
                  {%- endfor %}
                </ul>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          You have not added anyone so far in your team
        </dd>
            {% endif %}
            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/add-collaborators">
            Change<span class="govuk-visually-hidden">  team members</span>
              </a>
              {% endif %}
            </dd>
          </div>
        </dl>
        
{% endblock %}
{# END--------------------Project and colleagues-----------------#}
{# START----------------------Upload pricing schedules and any other supporting documents #}

{%block Upload_pricing_schedules%}
         <h2 class="govuk-heading-m">2. Upload pricing schedules and other supporting documents </h2>
         <dl class="govuk-summary-list">
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
            </dt>
            <dd class="govuk-summary-list__value">
            </dd>
            <dd class="govuk-summary-list__actions">
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <p>Upload your pricing schedule</p>
        </dt>
            {% if(document !=undefined and document !=null and document | length > 0) %}
              <dd class="govuk-summary-list__value">
                <ul class="govuk-list govuk-list--none">
                  {% for doc in document %}
                    <li>
                       <span class="govuk-body-s">{{doc}}</span>
                       </li>
                  {%- endfor %}
                </ul>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">  
          
        </dd>
            {% endif %}

            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/upload-attachment">
            Change<span class="govuk-visually-hidden"> pricing schedule</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__value">
          <p>Upload your terms and conditions and other documents</p>
        </dt>
            {# <dd class="govuk-summary-list__value"> #}
              {% if(documents !=undefined and documents !=null and documents | length > 0) %}
              <dd class="govuk-summary-list__value">
                <ul class="govuk-list govuk-list--none">
                  {% for doc in documents %}
                    <li>
                       <span class="govuk-body-s">{{doc}}</span>
                       </li>
                  {%- endfor %}
                </ul>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">          
        </dd>
            {% endif %}
            {# </dd> #}
            <dd class="govuk-summary-list__actions">
            {% if(eventStatus != "published")%}
              <a class="govuk-link" href="/rfp/upload-doc">
            Change<span class="govuk-visually-hidden"> uploaded document</span>
              </a>
              {% endif %}
            </dd>
          </div>
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
          <p>Confirm if you require a contracted out service or a supply of resource</p>
        </dt>
            {% if(ir35 !=undefined and  ir35 !=null and ir35 | length > 0) %}
              <dd class="govuk-summary-list__value">
                <p class="govuk-body-s">
                  {{ ir35 | safe }}
                </p>
              </dd>
            {% else %}
              <dd class="govuk-summary-list__value">
          
        </dd>
            {% endif %}
            <dd class="govuk-summary-list__actions">
            {% if eventStatus != "published"%}
              <a class="govuk-link" href="/rfp/IR35">
            Change<span class="govuk-visually-hidden"> if you require a contracted out service or a supply of resource</span>
              </a>
              {% endif %}
            </dd>
          </div>
        </dl>
{% endblock %}
{# END----------------------Upload pricing schedules and any other supporting documents #}

{# START----------------------3 Add context to your further competition #}
{%block Section_3%}
    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full">
            <h2 class="govuk-heading-m">3. Add context {% if selectedeventtype=='DA' %}and requirements{%else%}and requirements{%endif%} </h2>
              <dl class="govuk-summary-list">
          <div class="govuk-summary-list__row">
            <dt class="govuk-summary-list__key">
            </dt>
            <dd class="govuk-summary-list__value">
            </dd>
            <dd class="govuk-summary-list__actions">
            </dd>
          </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__value">
                      <h3><strong>Terms and acronyms (Optional) </strong></h3>
                      <br />
                      {% if(termAndAcr !=undefined and  termAndAcr !=null and termAndAcr | length > 0) %}
                      {% for data in termAndAcr %}
                      <p>Term or acronym</p><p>Definition</p><br />
                      {% endfor%}
                      {%endif%}
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <br /><br /><br />
                      {% if(termAndAcr !=undefined and  termAndAcr !=null and termAndAcr | length > 0 )%}
                      {% for data in termAndAcr %}
                      <p>{{data.value}} </p><p>{{data.text}}</p><br />
                      {% endfor%}
                      {%endif%}
                      {#<p>Expression or acronym answer</p><p>Definition answer</p><br />
                      <p>Expression or acronym answer</p><p>Definition answer</p><br />#}
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if eventStatus != "published"%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 3">
                          Change<span class="govuk-visually-hidden"> Term or acronym</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__value">
                      <h3><strong>Background to your procurement</strong></h3><br />
                      {% if(backgroundArr !=undefined and  backgroundArr !=null and backgroundArr | length > 0) %}
                      {% for data in backgroundArr%}
                      <p> Background information</p></br></br>
                      <p> Social value, economic and enviornmental benefits</p>
                      {% endfor %}
                      {%endif%}
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <br /><br /><br />
                      {% if(backgroundArr !=undefined and  backgroundArr !=null and backgroundArr | length > 0) %}
                      {% for data in backgroundArr%}
                      <p>{{data.v1}}</p><br /><br />
                      <p>{{data.v2}}</p>
                      {% endfor %}
                      {%endif%}
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if eventStatus != "published"%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 4">
                          Change<span class="govuk-visually-hidden"> Background to your procurement</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__key">
                      <p> The business problem you need to solve</p></br>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <p> {{businessProbAns}} </p>
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if eventStatus != "published"%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 5">
                          Change<span class="govuk-visually-hidden">The business problem you need to solve</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__value">
                      <h3><strong> The people who will use your product or service (optional)</strong></h3><br />
                      {% if keyUser !=undefined and  keyUser !=null and keyUser | length > 0 %}
                      {% for item in keyUser %}
                      <p> Service user type</p><p>Details about them</p><br />
                      {% endfor %}
                      {%endif%}
                      {#<p> Key user</p><p>definition</p><br />#}
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <br /><br /><br /><br />
                      {% if keyUser !=undefined and  keyUser !=null and keyUser | length > 0 %}
                      {% for item in keyUser %}
                      <p> {{item.value}}</p><p>{{item.text}}</p><br />
                      {% endfor %}
                      {%endif%}
                      {#<p> Key user answer</p><p>definition answer</p><br />#}
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 6">
                          Change<span class="govuk-visually-hidden"> The people who will use your product or service</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__key">
                      <p>Work done so far (optional)</p>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <p>{{workcompletedsofar}}</p>
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 7">
                          Change<span class="govuk-visually-hidden"> Work done so far</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__key">
                      <p>Which phase the project is in</p>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <p>{{currentphaseofproject}}</p>
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 8">
                          Change<span class="govuk-visually-hidden"> Which phase the project is in</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__key">
                      <p>
                        Which phases of the project you need resource for
                        </p>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      {% if phaseResource !=undefined and phaseResource !=null and phaseResource | length >0 %}
                      {% for data in phaseResource %}
                      <p>{{data}}</p>
                      {% endfor %}
                      {%endif%}
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 9">
                          Change<span class="govuk-visually-hidden"> Which phases of the project you need resource for</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__value">
                      <h3><strong>
                        {# Expected duration of the project  #}
                       How long the project will last
                        </strong></h3><br />
                      <p>The date you want the project to start</p><br />
                      <p>
                        How long you think the project will run for (optional)
                        </p><br />
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <br /><br /><br />
                      <p>{{startdate}}</p><br /><p>{{indicativedurationYear}} &nbsp; &nbsp; {{indicativedurationMonth}} &nbsp; &nbsp; {{indicativedurationDay}}</p><br />
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 10">
                          Change<span class="govuk-visually-hidden"> How long you think the project will run for</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__value">
                     <h3> <strong>Who the buying organisation is (optional)</strong></h3><br />
                      <p>Name of the organisation doing the procurement</p><br /><p>Name of the organisation using the products or services</p>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <br /><br /><br />
                      <p>{{buyingorg1}}</p><br /><br /><p>{{buyingorg2}}</p>
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 11">
                          Change<span class="govuk-visually-hidden">Name of the organisation doing the procurement</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__key">
                      <p>Describe any pre-market engagement you've done (optional) </p>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <p> {{summarize}}</p>
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if eventStatus != "published"%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 14">
                          Change<span class="govuk-visually-hidden">the Description of any pre-market engagement you've done</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__key">
                      <p>
                        Choose if this will be a new, replacement or expanded service or product
                        </p>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <p> {{newreplace}}</p>
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 15">
                          Change<span class="govuk-visually-hidden">if this will be a new, replacement or expanded service or product</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
                  <div class="govuk-summary-list__row">
                  <dt class="govuk-summary-list__key">
                      <p>Tell us if there is an existing supplier</p>
                  </dt>
                  <dd class="govuk-summary-list__value">
                      <p> {{incumbentoption}}</p><p>{{suppliername}}</p>
                  </dd>
                  <dd class="govuk-summary-list__actions">
                      {% if(eventStatus != "published")%}
                      <a class="govuk-link" href="/rfp/questions?agreement_id=RM6263&proc_id={{proc_id}}&event_id={{event_id}}&id=Criterion 3&group_id=Group 16">
                          Change<span class="govuk-visually-hidden">if there is an existing supplier</span>
                      </a>
                      {% endif %}
                  </dd>
                  </div>
