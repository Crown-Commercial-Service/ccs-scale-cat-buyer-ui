                                        class="govuk-link"
                                        href="/rfp/questions?agreement_id={{ agreementId_session }}&proc_id={{ proc_id }}&event_id={{ event_id }}&id=Criterion 3&group_id=Group 4">
                                        Change<span class="govuk-visually-hidden">project name</span>
                                      </a>
                                    {% endif %}
                                  </dd>
                                </div>
                                {% if(backgroundArr !=undefined and  backgroundArr !=null and backgroundArr | length > 0) %}
