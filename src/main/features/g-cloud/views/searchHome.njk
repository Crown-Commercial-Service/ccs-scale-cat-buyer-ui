            <label class="govuk-label" for="q">
              <strong>Search agreements</strong>
            </label>
            <p>Enter one or multiple keywords to search</p>
            <form class="search-form-dmp" method="GET">
              <div class="govuk-form-group">
                <div class="govuk-input__wrapper"><input class="govuk-input govuk-input--width-8" id="q" name="q" type="text" spellcheck="false">
                  <button type="submit" class="govuk-input__suffix button" aria-hidden="true">
                    Search
                  </button>
                </div>
              </div>

            </form>
          </div>
        </div>
      </div>
    </div>

    <div class="govuk-grid-row">
      <div class="govuk-grid-column-one-third">
        <h2 class="govuk-heading-m">Choose Category</h2>
        <h3 class="govuk-heading-s">All Categories</h3>
        <ul class="govuk-list">
         {% if serviceCategory.length >0 %}
         {% for categories in serviceCategory %}
          <li>
            <a class="govuk-link" href="#">{{categories}}</a>
          </li>
          {%endfor%}
          {%endif%}
        </ul>
      </div>
      <div class="govuk-grid-column-two-thirds">
        <div>
          <span class="govuk-body govuk-!-font-size-36">200</span> results found containing <strong>[keyword]</strong> in <strong>[category]</strong> where <strong>[Filter]</strong> is <strong>[Filter Value]</strong> and where <strong>[Filter]</strong> is <strong>[Filter Value]</strong>
