                    <dt class="govuk-summary-list__key">
                      Security services type
                    </dt>
                    <dd class="govuk-summary-list__value">
                      <ul class="govuk-list govuk-list--bullet">
                        {%if serviceDetails.securityTestingWhat !=null and serviceDetails.securityTestingWhat.length >0%}
                        {%for securityTestingWhat in serviceDetails.securityTestingWhat%}
                        <li>{{securityTestingWhat | replace("_", " ") | capitalize}}</li>
                        {%endfor%}
                        {%endif%}
