{% extends "template.njk" %}

{% if agreementId_session == 'RM6187' %}
{% set footerSupportLayer = true %}
{% set gcloudHelperLayer = true %}
{% endif %}

{% from "components/releatedContent.njk" import CCSReleatedContent %}
{% from "macros/csrf.njk" import csrfProtection %}

{% set title -%}
{{serviceDetails.serviceName}} | Contract Award Service
{%- endset %}
{% set url -%}
/projects/create-or-choose?lotId={{lotId}}&agreementLotName={{agreementLotName}}
{%- endset %}

{% block breadCrumb %}
{{ CCSBreadcrumbs({
  items: [
  {
    "text": "Dashboard",
    "href": "/dashboard"
  },
  {
    "text": "Choose a commercial agreement",
    "href": "/projects/choose-agreement"
  },
  {
    "text": "Procurement overview",
    "href": url
  },
  {
    "text": "Start a new search",
    "href": "/g-cloud/choose-category"
  },
  {
    "text": "Choose a category",
    "href": "/g-cloud/choose-category"
  },
  {
    "text": "Search results",
    "href": returnto | trim
  },
  {
    "text": serviceDetails.serviceName,
    "href": "#"
  }
  ]
}) }}
{% endblock %}

{% block content %}
<main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">
  {%block Page_Title_Section_1%}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <span class="govuk-caption-l">{{serviceDetails.supplierName}}</span>
      <h1 class="govuk-heading-l">{{serviceDetails.serviceName}}</h1>
    </div>
  </div>
  {%endblock%}

  {%block Data_Result_Section_2%}
  <div class="govuk-grid-row govuk-!-margin-bottom-9">
    <div class="govuk-grid-column-two-thirds">
      <p class="govuk-body">{{serviceDetails.serviceDescription| striptags(true) | escape | nl2br}}</p> 

      <h2 class="govuk-heading-m">Features</h2>
      <ul class="govuk-list govuk-list--bullet">
        {%if serviceDetails.serviceFeatures !=null and serviceDetails.serviceFeatures.length >0%}
        {%for feature in serviceDetails.serviceFeatures%}
        <li>{{feature}}</li>
        {%endfor%}
        {%endif%}
      </ul>


      <h2 class="govuk-heading-m">Benefits</h2>
      <ul class="govuk-list govuk-list--bullet">
        {%if serviceDetails.serviceBenefits !=null and serviceDetails.serviceBenefits.length >0%}
        {%for serviceBenefits in serviceDetails.serviceBenefits%}
        <li>{{serviceBenefits}}</li>
        {%endfor%}
        {%endif%}
      </ul>

    </div>

    <div class="govuk-grid-column-one-third">
    <div id="meta">
      {%if serviceDetails.priceMin !=null and serviceDetails.priceMin.length >0%}
        <h2 class="govuk-visually-hidden">Pricing</h2>
        <p class="govuk-!-font-weight-bold govuk-!-margin-bottom-1">
          £{{serviceDetails.priceMin}}
          {%if serviceDetails.priceMax !=null and serviceDetails.priceMax.length >0%}
               to £{{serviceDetails.priceMax}}
          {%endif%}

          {%if serviceDetails.priceUnit !=null and serviceDetails.priceUnit.length >0%}            
                {{serviceDetails.priceUnit | lower}}
          {%endif%}
          {%if serviceDetails.priceInterval !=null and serviceDetails.priceInterval.length >0%}
           {{serviceDetails.priceInterval | lower}}
           {%endif%}
        </p>
      {%endif%}
  <ul class="govuk-list govuk-body-s">
    {%if serviceDetails.educationPricing %}
    <li>
        Education pricing available
    </li>
    {% endif %}
    {%if serviceDetails.freeVersionTrialOption %}
    {%if serviceDetails.freeVersionLink !=null %}
    <li>
        <a href="{{serviceDetails.freeVersionLink}}" class="govuk-link" rel="external noopener noreferrer" target="_blank">Free trial available</a>
    </li>
     {% endif %}
   {% endif %}
