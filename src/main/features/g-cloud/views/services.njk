                  </div>
                  {%if serviceDetails.ongoingSupport %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Types of service supported
                    </dt>
                    <dd class="govuk-summary-list__value">
                      <ul class="govuk-list govuk-list--bullet">
                        {%if serviceDetails.ongoingSupportServices !=null and serviceDetails.ongoingSupportServices.length >0%}
                        {%for ongoingSupportServices in serviceDetails.ongoingSupportServices%}
