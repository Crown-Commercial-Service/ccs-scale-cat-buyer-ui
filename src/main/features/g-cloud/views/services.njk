          {%block Data_Result_Section_3%}
          <div class="govuk-grid-row" id="service-attributes">
            <div class="govuk-grid-column-full">
             
             {%if serviceDetails.lot =='cloud-support' %}
                <div class="scroll-tracking" id="01-planning">
                <h2 id="planning" class="govuk-heading-m app-summary-list-heading">Planning</h2>
                <dl class="govuk-summary-list app-govuk-summary-list app-govuk-summary-list--top-border govuk-!-margin-bottom-8">

                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Planning service
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{ "Yes" if serviceDetails.planningService  else "No" }}
                    </dd>
                  </div>
                  {%if serviceDetails.planningService %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      How the planning service works
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{serviceDetails.planningServiceDescription| striptags(true) | escape | nl2br}}
                    </dd>
                  </div>
                  {% endif %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Planning service works with specific services
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{ "Yes" if serviceDetails.planningServiceCompatibility  else "No" }}
                    </dd>
                  </div>
                  {%if serviceDetails.planningServiceCompatibility %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Hosting or software services the planning service works with
                    </dt>
                    <dd class="govuk-summary-list__value">
                      <ul class="govuk-list govuk-list--bullet">
                        {%if serviceDetails.planningServiceCompatibilityList !=null and serviceDetails.planningServiceCompatibilityList.length >0%}
                        {%for planningServiceCompatibilityList in serviceDetails.planningServiceCompatibilityList%}
                        <li>{{planningServiceCompatibilityList}}</li>
                        {%endfor%}
                        {%endif%}
                      </ul>
                    </dd>
                  </div>
                  {% endif %}
                </dl>
              </div>

              <div class="scroll-tracking" id="02-training">
                <h2 id="training" class="govuk-heading-m app-summary-list-heading">Training</h2>
                <dl class="govuk-summary-list app-govuk-summary-list app-govuk-summary-list--top-border govuk-!-margin-bottom-8">

                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Training service provided
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{ "Yes" if serviceDetails.training  else "No" }}
                    </dd>
                  </div>
                  {%if serviceDetails.training %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      How the training service works
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{serviceDetails.trainingDescription| striptags(true) | escape | nl2br}}
                    </dd>
                  </div>
                  {% endif %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Training is tied to specific services
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{ "Yes" if serviceDetails.trainingServiceSpecific  else "No" }}
                    </dd>
                  </div>
                  {%if serviceDetails.trainingServiceSpecific %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Services the training service works with
                    </dt>
                    <dd class="govuk-summary-list__value">
                      <ul class="govuk-list govuk-list--bullet">
                        {%if serviceDetails.trainingServiceSpecificList !=null and serviceDetails.trainingServiceSpecificList.length >0%}
                        {%for trainingServiceSpecificList in serviceDetails.trainingServiceSpecificList%}
                        <li>{{trainingServiceSpecificList}}</li>
                        {%endfor%}
                        {%endif%}
                      </ul>
                    </dd>
                  </div>
                  {% endif %}
                </dl>
              </div>

              <div class="scroll-tracking" id="03-setup-and-migration">
                <h2 id="setup-and-migration" class="govuk-heading-m app-summary-list-heading">Setup and migration</h2>
                <dl class="govuk-summary-list app-govuk-summary-list app-govuk-summary-list--top-border govuk-!-margin-bottom-8">

                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Setup or migration service available
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{ "Yes" if serviceDetails.setupAndMigrationService  else "No" }}
                    </dd>
                  </div>
                  {%if serviceDetails.setupAndMigrationService %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      How the setup or migration service works
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{serviceDetails.setupAndMigrationServiceDescription| striptags(true) | escape | nl2br}}
                    </dd>
                  </div>
                  {% endif %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Setup or migration service is for specific cloud services
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{ "Yes" if serviceDetails.setupAndMigrationServiceSpecific  else "No" }}
                    </dd>
                  </div>
                  {%if serviceDetails.setupAndMigrationServiceSpecific %}
                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      List of supported services
                    </dt>
                    <dd class="govuk-summary-list__value">
                      <ul class="govuk-list govuk-list--bullet">
                        {%if serviceDetails.setupAndMigrationServiceSpecificList !=null and serviceDetails.setupAndMigrationServiceSpecificList.length >0%}
                        {%for setupAndMigrationServiceSpecificList in serviceDetails.setupAndMigrationServiceSpecificList%}
                        <li>{{setupAndMigrationServiceSpecificList}}</li>
                        {%endfor%}
                        {%endif%}
                      </ul>
                    </dd>
                  </div>
                  {% endif %}
                </dl>
              </div>

              <div class="scroll-tracking" id="04-quality-assurance-and-performance-testing">
                <h2 id="quality-assurance-and-performance-testing" class="govuk-heading-m app-summary-list-heading">Quality assurance and performance testing</h2>
                <dl class="govuk-summary-list app-govuk-summary-list app-govuk-summary-list--top-border govuk-!-margin-bottom-8">

                  <div class="govuk-summary-list__row">
                    <dt class="govuk-summary-list__key">
                      Quality assurance and performance testing service
                    </dt>
                    <dd class="govuk-summary-list__value">
                      {{ "Yes" if serviceDetails.QAAndTesting  else "No" }}
